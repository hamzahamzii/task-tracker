{
  "name": "task-timer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production && yarn run copy:htaccess",
    "lint": "vue-cli-service lint",
    "build:modern": "vue-cli-service build --modern && yarn run copy:htaccess",
    "build:push": "yarn build && git add . && git commit -m 'prod master' && git push",
    "build:staging": "vue-cli-service build --mode staging && yarn run copy:htaccess",
    "build:staging:push": "yarn build:staging && git add . && git commit -m 'prod staging' && git push",
    "copy:htaccess": "copyfiles --flat htaccess/.htaccess dist",
    "lint:fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "apexcharts": "^3.26.0",
    "axios": "^0.21.1",
    "cookie": "^0.3.1",
    "copyfiles": "^2.4.1",
    "core-js": "^3.6.5",
    "firebase": "^8.2.5",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "register-service-worker": "^1.7.1",
    "sweetalert2": "^10.15.5",
    "tiptap": "^1.32.2",
    "tiptap-extensions": "^1.35.2",
    "vue": "^2.6.11",
    "vue-apexcharts": "^1.6.0",
    "vue-audio-recorder": "^3.0.1",
    "vue-cropperjs": "^3.0.0",
    "vue-json-excel": "^0.3.0",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.2.0",
    "vue-web-cam": "^1.9.0",
    "vuetify": "^2.4.2",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^2.5.4",
    "vuex-shared-mutations": "^1.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-pwa": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.0",
    "@vue/cli-plugin-vuex": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  }
}
